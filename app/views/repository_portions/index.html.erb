<h1>Listing repository portions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url part</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @repository_portions.each do |repository_portion| %>
  <tr>
    <td><%= repository_portion.name %></td>
    <td><%= repository_portion.url_part %></td>
    <td><%= link_to 'Show', repository_portion %></td>
    <td><%= link_to 'Edit', edit_repository_portion_path(repository_portion) %></td>
    <td><%= link_to 'Destroy', repository_portion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

