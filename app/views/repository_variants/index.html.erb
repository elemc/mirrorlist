<h1>Listing repository_variants</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @repository_variants.each do |repository_variant| %>
  <tr>
    <td><%= repository_variant.name %></td>
    <td><%= repository_variant.url %></td>
    <td><%= link_to 'Show', repository_variant %></td>
    <td><%= link_to 'Edit', edit_repository_variant_path(repository_variant) %></td>
    <td><%= link_to 'Destroy', repository_variant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

