<h1>Listing stats</h1>

<table>
  <tr>
    <th>Country code</th>
    <th>City</th>
    <th>Repo</th>
    <th>Arch</th>
    <th>Count</th>
    <!--<th></th>
    <th></th>
    <th></th>-->
  </tr>

<% @stats.each do |stat| %>
  <tr>
    <td><%= stat.country %></td>
    <td><%= stat.city %></td>
    <td><%= stat.repo %></td>
    <td><%= stat.arch %></td>
    <td><%= stat.count %></td>
    <!--
    <td><%= link_to 'Show', stat %></td>
    <td><%= link_to 'Edit', edit_stat_path(stat) %></td>
    <td><%= link_to 'Destroy', stat, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
  </tr>
<% end %>
</table>

